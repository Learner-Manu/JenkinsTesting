pipeline {
  agent any
  stages {
    stage('Pull Update') {
      steps {
        echo 'pull update'
      }
    }
    stage("Browser Test"){
      parallel {
      stage('Chrome') {
      steps {
        echo 'chrome test'
      }
    }
    stage('Firefox') {
      steps {
        echo 'firefox test'
      }
    }
    stage('Internet Explorer') {
      steps {
        echo 'ie test'
      }
    }
  }
}